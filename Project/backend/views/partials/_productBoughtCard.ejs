<article class="articulo">
    <div class="articulo-imagen" onclick="window.location='/products/detail/<%= order.product.id_product %>'"><img src="/images/products/<%= order.product.image %>" alt=<%= order.product.name %>></div>
    <div class="articulo-info">
        <div class="articulo-precio">
            <h3 class="texto-articulo"><%= order.product.name %></h3>
            <h3 class="texto-precio">U$S <%= order.product.price %></h3>
        </div>
        <div class="articulo-opciones">
            <ul>
                <li>
                    <form id="" action="" method="">
                        <a class="cart-eliminar" href="">Fecha de compra:</a>
                    </form>
                </li>
                <li><a href=""><%= order.last_modified.getDate() + "/" + order.last_modified.getMonth()+1 + "/"+ order.last_modified.getFullYear() + " " 
                + order.last_modified.getHours()+ ":" + order.last_modified.getMinutes() + ":" + order.last_modified.getSeconds() %></a></li>
            </ul>
        </div>
    </div>
</article>